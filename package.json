{
  "name": "hexa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "clear": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "prepare": "chmod +x setup.sh && sh ./setup.sh",
    "postinstall": "jetify",
    "android": "react-native run-android",
    "android-release": "cd android && ./gradlew app:assembleRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean:android": "cd android/; ./gradlew clean; cd ..;",
    "log:android": "react-native log-android",
    "log:ios": "react-native log-ios",
    "log:processes": "lsof -i tcp:8081",
    "log:environment-info": "react-native info",
    "snapshot:environment-info": "yarn log:environment-info | pbcopy",
    "androidDevelopmentDebug": "ENVFILE=.env.development react-native run-android --variant=developmentDebug --appIdSuffix=development",
    "androidDevelopmentRelease": "ENVFILE=.env.development react-native run-android --variant=developmentRelease --appIdSuffix=development",
    "androidStagingDebug": "ENVFILE=.env.staging react-native run-android --variant=stagingDebug --appIdSuffix=staging",
    "androidStagingRelease": "ENVFILE=.env.staging react-native run-android --variant=stagingRelease --appIdSuffix=staging",
    "androidProductionDebug": "ENVFILE=.env.production react-native run-android --variant=productionDebug",
    "androidProductionRelease": "ENVFILE=.env.production react-native run-android --variant=productionRelease"
  },
  "xcodeSchemes": {
    "Debug": [
      "HEXA Stage",
      "HEXA Dev"
    ],
    "Release": [
      "HEXA Stage",
      "HEXA Dev"
    ],
    "projectDirectory": "iOS"
