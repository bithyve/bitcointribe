{
  "name": "hexa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "clear": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "prepare": "husky install && chmod +x setup.sh && sh ./setup.sh",
    "postinstall": "patch-package && jetify",
    "android": "react-native run-android",
    "android-release": "cd android && ./gradlew app:assembleRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean:android": "cd android/; ./gradlew clean; cd ..;",
    "clean:ios": "cd ios/ && rm -rf build/ Pods/ Podfile.lock && cd ..",
    "clean:watchman": "watchman watch-del-all",
    "clean:node": "rm -rf node_modules/ && yarn cache clean",
    "log:android": "react-native log-android",
    "log:ios": "react-native log-ios",
    "log:processes": "lsof -i tcp:8081",
    "log:environment-info": "react-native info",
    "snapshot:environment-info": "yarn log:environment-info | pbcopy",
    "androidDevelopmentDebug": "ENVFILE=.env react-native run-android --variant=developmentDebug --appIdSuffix=development",
    "androidDevelopmentRelease": "ENVFILE=.env react-native run-android --variant=developmentRelease --appIdSuffix=development",
    "androidStagingDebug": "ENVFILE=.env react-native run-android --variant=stagingDebug --appIdSuffix=staging",
    "androidStagingRelease": "ENVFILE=.env react-native run-android --variant=stagingRelease --appIdSuffix=staging",
    "androidProductionDebug": "ENVFILE=.env react-native run-android --variant=productionDebug",
    "androidProductionRelease": "ENVFILE=.env react-native run-android --variant=productionRelease",
    "deep-clean": "yarn clear && yarn clean:watchman && yarn clean:node && yarn clean:ios && yarn clean:android && yarn install"
  },
  "xcodeSchemes": {
    "Debug": [
      "HEXA Stage",
      "HEXA Dev"
    ],
    "Release": [
      "HEXA Stage",
      "HEXA Dev"
    ],
    "projectDirectory": "iOS"
  },
